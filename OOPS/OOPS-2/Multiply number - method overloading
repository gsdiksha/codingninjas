import java.util.Scanner;
class Helper {
	// Method 1
	// Multiplication of 2 numbers
	// Method 2
	// // Multiplication of 3 numbers
	// Method 1: Multiplication of 2 numbers
    public static int multiply(int a, int b) {
        return a * b;
    }

    // Method 2: Multiplication of 3 numbers
    public static int multiply(int a, int b, int c) {
        return a * b * c;
    }
}

// Class 2
class Solution {

	// Main driver method
	public static void main(String[] args)
	{
        // Take input using Scanner
        Scanner scanner = new Scanner(System.in);
        // System.out.println("Enter two numbers:");
        int num1 = scanner.nextInt();
        int num2 = scanner.nextInt();
		int num3 = scanner.nextInt();

		int result1 = Helper.multiply(num1, num2);
        System.out.println(result1);

		// Call Method 2 and print the output
        int result2 = Helper.multiply(num1, num2, num3);
        System.out.println(result2);

        // Close the scanner
        scanner.close();
		// Take input using Scanner and print the output .
	}
}

simple input :
5
3
7
Sample output :
15
105
